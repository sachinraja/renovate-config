{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [":semanticCommits", ":disableDevDependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchPackageNames": ["typescript"],
      "automerge": false
    }
  ]
}
